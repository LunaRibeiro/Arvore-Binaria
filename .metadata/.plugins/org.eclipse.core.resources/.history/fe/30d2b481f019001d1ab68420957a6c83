package document.assembler;

public class No<TYPE> {

	private TYPE value;
	private No<TYPE> left;
	private No<TYPE> right;
	
	public No(TYPE newValue) {
		 this.value = newValue;
		 this.left = null;
		 this.right = null; 
	}

	public TYPE getValue() {
		return value;
	}

	public void setValue(TYPE value) {
		this.value = value;
	}

	public No<TYPE> getLeft() {
		return left;
	}

	public void setLeft(No<TYPE> left) {
		this.left = left;
	}

	public No<TYPE> getRight() {
		return right;
	}

	public void setRight(No<TYPE> right) {
		this.right = right;
	}
}
